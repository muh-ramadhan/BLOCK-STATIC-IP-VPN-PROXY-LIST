Mikrotik Console
# Add Filter Rule Drop Static IP VPN PROXY
/ip firewall filter add chain=forward dst-address-list=LIST_STATIC_IP_VPN_PROXY action=drop comment="DROP ALL LIST_STATIC_IP_VPN_PROXY"

# Add Static IP To Address List LIST_STATIC_IP_VPN_PROXY
/ip firewall address-list add list=LIST_STATIC_IP_VPN_PROXY address=128.1.186.9
/ip firewall address-list add list=LIST_STATIC_IP_VPN_PROXY address=128.1.172.155
/ip firewall address-list add list=LIST_STATIC_IP_VPN_PROXY address=128.14.65.247
